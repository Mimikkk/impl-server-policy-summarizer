{
  "compilerOptions": {
    "lib": [
      "deno.window",
      "deno.ns",
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "noErrorTruncation": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "name": "@nimir/app-ui",
  "version": "0.0.0",
  "exports": "./src/app.ts",
  "tasks": {
    "dev": {
      "description": "Run the ui application",
      "command": "deno run -A npm:@rsbuild/core@^1.4.1 dev"
    },
    "build": {
      "description": "Build the ui application",
      "command": "deno run -A npm:@rsbuild/core@^1.4.1 build"
    },
    "test": {
      "description": "Test the ui application",
      "command": "deno test"
    }
  },
  "imports": {
    "@components/": "./src/core/components/",
    "@hooks/": "./src/core/hooks/",
    "@utilities/": "./src/core/utilities/",
    "@services/": "./src/core/services/",
    "@features/": "./src/features/",
    "react": "npm:react@19.1.0",
    "react-dom": "npm:react-dom@19.1.0",
    "@tanstack/react-query": "npm:@tanstack/react-query@5.81.2",
    "@rsbuild/core": "npm:@rsbuild/core@^1.4.1",
    "@rsbuild/plugin-react": "npm:@rsbuild/plugin-react@^1.3.2",
    "@tailwindcss/postcss": "npm:@tailwindcss/postcss@^4.1.10",
    "tailwindcss": "npm:tailwindcss@^4.1.10",
    "@headlessui/react": "npm:@headlessui/react@^2.2.4",
    "@tanstack/react-router": "npm:@tanstack/react-router@^1.123.2",
    "@tanstack/react-router-devtools": "npm:@tanstack/react-router-devtools@^1.123.2",
    "@tanstack/router-plugin": "npm:@tanstack/router-plugin@^1.123.2",
    "lucide-react": "npm:lucide-react@^0.525.0",
    "clsx": "npm:clsx@^2.1.1",
    "ky": "npm:ky@^1.8.1",
    "shiki": "npm:shiki@^3.7.0",
    "@types/react": "npm:@types/react@^19.1.8",
    "@types/react-dom": "npm:@types/react-dom@^19.1.6",
    "prettier": "npm:prettier@^3.6.2"
  }
}
